<woo-options>
    <dsn>mysql:host=localhost;dbname=imei-service</dsn>
<control>
    <view>main</view>
    <view status="CMD_OK">main</view>
    <view status="CMD_ERROR">error</view>

    <command name="News">
        <view>news</view>
        <status value="CMD_OK">
            <forward>NewsPrint</forward>
        </status>
    </command>

    <command name="NewsPrint">
        <view>newsPrint</view>
    </command>

    <command name="Guestbook">
        <view>guestbook</view>
    </command>

    <command name="Contacts">
        <view>contacts</view>
    </command>

    <command name="ListVenues">
        <view>listvenues</view>
    </command>

    <command name="QuickAddVenue">
        <classalias name="AddVenue" />
        <view>quickadd</view>
    </command>

    <command name="AddVenue">
        <view>addvenue</view>
        <status value="CMD_OK">
          <forward>AddSpace</forward>
          <!--<forward>AddVenue</forward> -->
        </status>
    </command>

    <command name="AddSpace">
        <view>addspace</view>
        <status value="CMD_OK">
            <forward>ListVenues</forward>
        </status>
    </command>
</control>
</woo-options>
