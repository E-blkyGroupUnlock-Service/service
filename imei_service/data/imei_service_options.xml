<woo-options>
    <dsn>mysql:host=localhost;dbname=imei-service</dsn>
<control>
    <view>main</view>
    <view status="CMD_OK">main</view>
    <view status="CMD_ERROR">error</view>

    <command name="News">
        <view>news</view>
        <status value="CMD_OK">
            <forward>NewsPrint</forward>
        </status>
    </command>

    <command name="NewsPrint">
        <view>newsPrint</view>
    </command>

    <command name="Guestbook">
        <view>guestbook</view>
        <status value="CMD_OK">
            <view>addMessageSuccess</view>
        </status>
    </command>

    <command name="Contacts">
        <view>contacts</view>
    </command>

    <command name="Unlock">
        <view>unlock</view>
        <status value="CMD_OK">
            <forward>UnlockDetails</forward>
        </status>
    </command>

    <command name="UnlockDetails">
        <view>unlockDetails</view>
    </command>

    <command name="Udid">
        <view>udid</view>
    </command>

    <command name="CarrierCheck">
        <view>carrierCheck</view>
    </command>

    <command name="FastCheck">
        <view>fastCheck</view>
    </command>

    <command name="BlacklistCheck">
        <view>blacklistCheck</view>
    </command>

    <command name="Faq">
        <view>faq</view>
    </command>

</control>
</woo-options>
